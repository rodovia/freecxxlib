#pragma once

#include "internals/btypes.h"
#include "type_traits"

_FCL_NAMESPACE_BEGIN

template<class _Ty> void swap(_Ty& item1, _Ty& item2) noexcept
{
    _Ty tmp = item2;
    item2 = item1;
    item1 = tmp;
}

template<class _Ty> 
constexpr remove_reference_t<_Ty>&& move(_Ty&& t) noexcept
{
    return static_cast<remove_reference_t<_Ty>&&>(t);
}

_FCL_NAMESPACE_END
